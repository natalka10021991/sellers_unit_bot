{
  "name": "wb-margin-bot",
  "version": "1.0.0",
  "type": "module",
  "description": "Telegram бот для расчета маржи товаров на Wildberries",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "dev:local": "NODE_ENV=development tsx watch src/index.ts",
    "build": "tsc",
    "build:prod": "NODE_ENV=production npm run build",
    "start": "NODE_ENV=production node dist/index.js",
    "start:dev": "NODE_ENV=development node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "precommit": "npm run typecheck && npm run build"
  },
  "keywords": [
    "telegram",
    "bot",
    "wildberries",
    "margin",
    "calculator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "grammy": "^1.21.1",
    "dotenv": "^16.3.1",
    "better-sqlite3": "^9.2.2",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
