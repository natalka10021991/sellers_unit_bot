# Telegram Bot Token (получи у @BotFather)
BOT_TOKEN=your_bot_token_here

# Wildberries API Token (получи на https://dev.wildberries.ru)
WB_API_TOKEN=your_wb_token_here

# Mini App URL (для продакшена)
MINI_APP_URL=https://mini-app-red-seven.vercel.app

# Окружение (development для локальной разработки, production для продакшена)
NODE_ENV=development

# API Server Port (для локальной разработки)
API_PORT=3000

# Лимиты
FREE_CALCULATIONS_LIMIT=5
SUBSCRIPTION_PRICE=149
SUBSCRIPTION_DAYS=30

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS=10
RATE_LIMIT_WINDOW_MS=60000

# Database Path (для локальной разработки)
DB_PATH=./data/bot.db

